{
  "name": "tanstarter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "dev:ws": "bun run server-ws.ts",
    "worker": "bun run src/worker.ts",
    "worker:dev": "bun --watch run src/worker.ts",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "test:video": "bun run scripts/test-video-generation.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check": "bun run format && bun run lint && bun run check-types",
    "db": "drizzle-kit",
    "db:push": "bunx drizzle-kit push",
    "db:reset": "bun run scripts/reset-db.ts",
    "deps": "bunx taze@latest -Ilw",
    "deps:major": "bunx taze@latest major -Ilw",
    "ui": "bunx shadcn@latest",
    "auth:secret": "bunx @better-auth/cli@latest secret",
    "auth:generate": "bunx @better-auth/cli@latest generate --config ./src/lib/auth/auth.ts --y --output ./src/lib/db/schema/auth.schema.ts && prettier --write ./src/lib/db/schema/auth.schema.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.136.18",
    "@tanstack/react-router-ssr-query": "^1.136.18",
    "@tanstack/react-start": "^1.136.18",
    "@types/cors": "^2.8.19",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.1",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crossws": "^0.4.1",
    "drizzle-orm": "^0.44.7",
    "fluent-ffmpeg": "^2.1.3",
    "lucide-react": "^0.554.0",
    "next-themes": "^0.4.6",
    "nitro": "3.0.1-alpha.1",
    "pg-boss": "^12.4.1",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.5.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vite": "npm:rolldown-vite@latest",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.136.18",
    "@types/bun": "^1.3.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
